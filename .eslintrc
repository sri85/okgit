{
    "extends": "eslint:recommended",
    "rules": {
      // enable additional rules
      "quotes": [
        "error",
        "double"
      ],
      "semi": [
        "error",
        "always"
      ]
    },
    "parser": "babel-eslint",
      "parserOptions": {
        "ecmaVersion": 6,
        "sourceType": "module",
        "ecmaFeatures": {
          "jsx": true
        }
      },
    "env": {
      "es6": true,
      "node": true
    }
  }